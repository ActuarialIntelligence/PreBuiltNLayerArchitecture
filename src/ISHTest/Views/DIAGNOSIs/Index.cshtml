@model IEnumerable<AML.Infrastructure.Data.EF.DIAGNOSI>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ISSUE_DATE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TREATMENT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.REMARKS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NURSE_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DOC_ID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ISSUE_DATE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TREATMENT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.REMARKS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NURSE_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DOC_ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
